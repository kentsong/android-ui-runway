<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.ui.kent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".demo.recyclerview.RecyclerViewActivity"/>
        <activity android:name=".demo.listview.ListViewActivity"/>
        <activity android:name=".demo.alarm.AlarmActivity"/>

        <receiver
            android:name=".demo.alarm.AlarmReceiver"
            android:enabled="true"
            android:exported="true" >
        </receiver>

    </application>

</manifest>