<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.ui.kent">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".demo.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".demo.recyclerview.RecyclerViewActivity"/>
        <activity android:name=".demo.listview.ListViewActivity"/>
        <activity android:name=".demo.alarm.AlarmActivity"/>
        <activity android:name=".demo.drawer.DrawerActivity"/>
        <activity android:name=".demo.tab.TabActivity"/>
        <activity android:name=".demo.indicator.IndicatorActivity"/>
        <activity android:name=".demo.cardview.CardActivity"/>
        <activity android:name=".demo.network.retrofit.RetrofitActivity"/>
        <activity android:name=".demo.network.okhttp.OKHttpActivity"/>
        <activity android:name=".demo.network.rxjava.RxJavaActivity"/>
        <activity android:name=".demo.eventbus.EventBusActivity"/>
        <activity android:name=".demo.widget.dialog.DialogActivity"/>
        <activity android:name=".demo.recyclerview.loadmore.LoadMoreActivity"/>
        <activity android:name=".demo.layout.contraint.ConstraintLayoutActivity"/>
        <activity android:name=".demo.layout.style.StyleSelectorActivity"/>
        <activity android:name=".demo.ui_response.UIResponseActivity"/>
        <activity android:name=".demo.recyclerview.multi_layer.MultiLayerActivity" />
        <activity android:name=".demo.widget.view.CustomizeViewActivity"/>

        <receiver
            android:name=".demo.alarm.AlarmReceiver"
            android:enabled="true"
            android:exported="true" >
        </receiver>

    </application>

</manifest>